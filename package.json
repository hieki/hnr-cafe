{
  "name": "hnr-cafe",
  "version": "1.0.0",
  "description": "hnr-cafe.",
  "private": true,
  "main": "build.js",
  "scripts": {
    "build": "npm run fmt && run-p build:*",
    "build:js": "webpack",
    "build:html": "ts-node --project ./tsconfig.node.json ./build.ts",
    "build:sounds": "cp -r ./client/sounds ./public/sounds",
    "fmt": "run-p eslint prettier",
    "eslint": "eslint -c .eslintrc.json './**/*.{js,ts}'",
    "eslint-fix": "eslint --fix -c .eslintrc.json './**/*.{js,ts}'",
    "prettier": "prettier -c './**/*.{js,ts}'",
    "prettier-fix": "prettier --write './**/*.{js,ts}'"
  },
  "author": "hieki",
  "license": "MIT",
  "devDependencies": {
    "@types/ejs": "^2.6.3",
    "@types/gtag.js": "0.0.0",
    "@types/node": "^11.13.8",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "ejs": "^2.6.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-ejs-js": "^0.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "ts-loader": "^5.4.4",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "dependencies": {}
}
