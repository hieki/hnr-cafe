{
  "name": "hnr-cafe",
  "version": "1.0.0",
  "description": "hnr-cafe.",
  "private": true,
  "main": "build.js",
  "scripts": {
    "build": "npm run fmt && run-p build:*",
    "build:js": "webpack",
    "build:html": "ts-node --project ./tsconfig.node.json ./build.ts",
    "build:sounds": "cp -r ./client/sounds ./public/sounds",
    "fmt": "run-p eslint prettier",
    "eslint": "eslint -c .eslintrc.json './**/*.{js,ts}'",
    "eslint-fix": "eslint --fix -c .eslintrc.json './**/*.{js,ts}'",
    "prettier": "prettier -c './**/*.{js,ts}'",
    "prettier-fix": "prettier --write './**/*.{js,ts}'"
  },
  "author": "hieki",
  "license": "MIT",
  "devDependencies": {
    "@types/ejs": "^2.6.3",
    "@types/gtag.js": "0.0.2",
    "@types/node": "^12.6.9",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "ejs": "^2.6.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-ejs-js": "^0.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {}
}
